{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "f1-block-library/single-card",
  "title": "Single Card",
  "category": "widgets",
  "parent": ["f1-block-library/manual-cards"],
  "editorScript": "file:../../build/single-card.js",
  "style": "file:../../build/query-cards.css",
  "editorStyle": "file:./single-card-editor.css",
  "attributes": {
    "url": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "id": {
      "type": "number"
    },
    "type": {
      "type": "string"
    },
    "opensInNewTab": {
      "type": "boolean",
      "default": false
    },
    "cardTitle": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    },
    "eyebrow": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    },
    "imageId": {
      "type": "number"
    },
    "imageAlt": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "alt",
      "default": ""
    }
  }
}
